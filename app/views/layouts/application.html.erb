<!DOCTYPE html>
<html>
<head>
  <title><%= @title %></title>
  <%= stylesheet_link_tag "custom", "portfolio", "other_works" %>
  <!--[if IE]><%= stylesheet_link_tag "ie" %><![endif]-->
  <%= javascript_include_tag 'jquery-1.6.2.min', 'slider_on_main', 'works' %>
  <%= csrf_meta_tag %>

</head>
<body>	
	<% if current_user %>
	<div id="user_nav">
	   Logged in as <%= current_user.email %>.
	   <%= link_to "Log out", log_out_path %>
	</div>
	 <% end %>
	
<% flash.each do |name, msg| %>
<%= content_tag :div, msg, :id => "flash_#{name}" %>
<% end %>

<%= yield %>

</body>
</html>
